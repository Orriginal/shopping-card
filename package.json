        "eslint-plugin-react-hooks": "^4.6.2",
        "install": "^0.13.0",
        "nodemon": "^3.1.2",
        "npm": "^10.8.1",
        "prettier": "^3.3.0",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.0"
    }
