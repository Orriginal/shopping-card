        "@testing-library/react": "^15.0.7",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.12",
        "@types/node": "^16.18.97",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "cors": "^2.8.5",
